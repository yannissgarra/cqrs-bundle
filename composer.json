{
    "name": "webmunkeez/cqrs-bundle",
    "type": "symfony-bundle",
    "description": "Command-Query-Responsibility-Segregation pattern made for Symfony.",
    "keywords": [
        "symfony",
        "cqrs",
        "command",
        "query",
        "event",
        "doctrine",
        "autowiring"
    ],
    "homepage": "https://github.com/yannissgarra/cqrs-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannis Sgarra",
            "email": "hello@yannissgarra.com",
            "homepage": "https://yannissgarra.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/orm": "^2.12",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/messenger": "^6.0",
        "symfony/uid": "^6.0",
        "symfony/validator": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/framework-bundle": "^6.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Webmunkeez\\CQRSBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webmunkeez\\CQRSBundle\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
