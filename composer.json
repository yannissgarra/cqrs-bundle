{
  "name": "webmunkeez/cqrs-bundle",
  "type": "symfony-bundle",
  "description": "Command-Query-Responsibility-Segregation pattern made for Symfony.",
  "keywords": [
    "symfony",
    "cqrs",
    "command",
    "query",
    "event",
    "messenger",
    "autowiring"
  ],
  "homepage": "https://github.com/yannissgarra/cqrs-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Yannis Sgarra",
      "email": "hello@yannissgarra.com",
      "homepage": "https://yannissgarra.com"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "symfony/config": "^5.4",
    "symfony/dependency-injection": "^5.4",
    "symfony/messenger": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/framework-bundle": "^5.4",
    "symfony/phpunit-bridge": "^5.4",
    "symfony/yaml": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "Webmunkeez\\CQRSBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Webmunkeez\\CQRSBundle\\Test\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
